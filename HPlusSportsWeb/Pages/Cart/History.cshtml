@page
@model HistoryModel
@{
    ViewData["Title"] = "Order History";
}

<section class="default">
    <h2>Order History</h2>


    <table class="table">
        <thead>
            <tr>
                <th>
                    Product ID
                </th>
                <th>
                    Product Name
                </th>
                <th>
                    Quantity
                </th>
                <th>
                    Size
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.OrderHistoryEntities)
            {
                <tr>
                    <td>
                        @item.Id
                    </td>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Quantity
                    </td>
                    <td>
                        @item.Size
                    </td>

                </tr>
            }
        </tbody>
    </table>
</section>